<table class="product_listing">
	<tr>
		<th colspan=2><a href="page.cgi?id=prodzandplanz/bug_versions.html&bug_id=[% bug.id %]">Same bug in other product versions</a></th>
	</tr>
	<tr>
		<th>Impacted versions</th>
		<th>Not impacted</th>
	</tr>
	[% FOREACH v = product_versions %]
	<tr>
		[% IF versions_map.$v %]
			<td><span [% IF bug_leader_id == versions_map.$v %]class="pap_bug_leader"[% END %]>[% v %]</span> (<a href="show_bug.cgi?id=[% versions_map.$v %]">Bug [% versions_map.$v %]</a>)</td>
			<td></td>
		[% ELSE %]
			<td></td>
			<td>[% v %]</td>
		[% END %]
	</tr>
	[% END %]
</table>
	